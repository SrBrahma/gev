{
  "name": "gev",
  "version": "2.20.0",
  "description": "Generates boilerplates",
  "keywords": [
    "generator",
    "template",
    "config",
    "environment",
    "setup",
    "creator",
    "starter",
    "base",
    "eslint",
    "tsconfig",
    "javascript",
    "project",
    "projects",
    "typescript",
    "script",
    "boilerplate"
  ],
  "homepage": "https://github.com/SrBrahma/gev#readme",
  "bugs": {
    "url": "https://github.com/SrBrahma/gev/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SrBrahma/gev.git"
  },
  "license": "ISC",
  "author": "SrBrahma",
  "type": "module",
  "exports": "./lib/main/index.js",
  "main": "./lib/main/index.js",
  "bin": "./lib/main/index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "files": [
    "lib",
    "semitemplates",
    "content"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf lib",
    "gev": "npm start",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "npx eslint \"src/**\"",
    "lint:fix": "npx eslint --fix \"src/**\"",
    "start": "ts-node-esm ./src/main/index.ts",
    "test": "echo \"Error: no test specified\"",
    "test:all": "npm run typecheck && npm run lint && npm run test",
    "postinstall": "husky install",
    "scripts:templates": "ts-node-esm ./scripts/generateTemplates.ts",
    "scripts:emptyDirs": "ts-node-esm ./scripts/makeEmptyDirsFile.ts",
    "pre-commit": "lint-staged && npm run scripts:emptyDirs && git add -A",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "add-dependencies": "1.1.0",
    "base-64": "1.0.0",
    "chalk": "5.1.2",
    "commander": "9.4.1",
    "cross-fetch": "3.1.5",
    "edit-json-file": "^1.7.0",
    "execa": "6.1.0",
    "fs-extra": "10.1.0",
    "globby": "^13.1.2",
    "inquirer": "^9.1.4",
    "latest-version": "7.0.0",
    "ora": "^6.1.2",
    "rimraf": "3.0.2",
    "semver-compare": "1.0.0",
    "signal-exit": "3.0.7",
    "typescript": "^4.8.4",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@types/base-64": "1.0.0",
    "@types/edit-json-file": "^1.7.0",
    "@types/fs-extra": "9.0.13",
    "@types/inquirer": "^9.0.2",
    "@types/node": "^18.11.4",
    "@types/rimraf": "^3.0.2",
    "@types/semver-compare": "1.0.1",
    "@types/signal-exit": "3.0.1",
    "@types/validate-npm-package-name": "^4.0.0",
    "eslint-config-gev": "^2.43.3",
    "husky": "^8.0.1",
    "pinst": "^3.0.0",
    "ts-node": "10.9.1"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
