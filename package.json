{
  "name": "gev",
  "version": "2.23.2",
  "description": "Generates boilerplates",
  "keywords": [
    "generator",
    "template",
    "setup",
    "creator",
    "starter",
    "project",
    "boilerplate"
  ],
  "homepage": "https://github.com/SrBrahma/gev#readme",
  "bugs": {
    "url": "https://github.com/SrBrahma/gev/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SrBrahma/gev.git"
  },
  "license": "ISC",
  "author": "SrBrahma",
  "type": "module",
  "exports": "./lib/main/index.js",
  "main": "./lib/main/index.js",
  "bin": "./lib/main/index.js",
  "files": [
    "lib",
    "semitemplates",
    "content"
  ],
  "scripts": {
    "build": "pnpm clean && tsc",
    "clean": "rimraf lib",
    "gev": "pnpm start",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint \"src/**\" --cache",
    "lint:fix": "eslint --fix \"src/**\"",
    "prettier": "prettier \"**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx}\"",
    "prettify": "pnpm lint:fix && pnpm prettier:fix",
    "start": "ts-node ./src/main/index.ts",
    "test": "echo \"Error: no test specified\"",
    "test:all": "pnpm typecheck && pnpm lint && pnpm test",
    "prepare": "husky install && pnpm build",
    "script:templates": "ts-node ./scripts/generateTemplates.ts",
    "script:emptyDirs": "ts-node ./scripts/makeEmptyDirsFile.ts",
    "pre-commit": ""
  },
  "lint-staged": {
    "{js,jsx,ts,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "add-dependencies": "1.1.0",
    "base-64": "1.0.0",
    "chalk": "5.1.2",
    "commander": "9.4.1",
    "cross-fetch": "3.1.5",
    "edit-json-file": "^1.7.0",
    "eslint-config-gev": "^3.1.0",
    "execa": "6.1.0",
    "fs-extra": "10.1.0",
    "globby": "^13.1.2",
    "inquirer": "^9.1.4",
    "latest-version": "7.0.0",
    "ora": "^6.1.2",
    "semver-compare": "1.0.0",
    "signal-exit": "3.0.7",
    "typescript": "^4.8.4",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "@types/base-64": "1.0.0",
    "@types/edit-json-file": "^1.7.0",
    "@types/fs-extra": "9.0.13",
    "@types/inquirer": "^9.0.2",
    "@types/node": "^18.11.4",
    "@types/semver-compare": "1.0.1",
    "@types/signal-exit": "3.0.1",
    "@types/validate-npm-package-name": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "13.0.3",
    "pinst": "^3.0.0",
    "prettier": "^2.8.1",
    "prettier-config-gev": "^1.1.2",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "type-fest": "^3.5.0"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "prettier": "prettier-config-gev"
}
